	<xs:element name="LocationPreselectionRequest">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="referenceLocation" type="Location">
					<xs:annotation>
						<xs:documentation>Reference location that serves as '1' in the requested 1:n or n:1</xs:documentation>
					</xs:annotation>
